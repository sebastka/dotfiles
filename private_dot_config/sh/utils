[ ! -s "$NVM_DIR/nvm.sh" ] || . "$NVM_DIR/nvm.sh"
[ ! -s "$CARGO_HOME/env" ] || . "$CARGO_HOME/env"
[ ! -s "$XDG_DATA_HOME/python/bin/activate" ] || . "$XDG_DATA_HOME/python/bin/activate"
[ ! -s "$RVM_PATH/scripts/rvm" ] || . "$RVM_PATH/scripts/rvm" # rvm pkg install openssl && rvm (re)install ruby-3.4.2 --with-openssl-dir=$RVM_PATH/usr
